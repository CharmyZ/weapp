
<!--pages/index/index.wxml-->
<view class="page">
  <view class="page-top">
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{imgUrls}}" wx:key="i" wx:for-index="i" wx:for-item="item">
        <swiper-item>
          <navigator url="{{item.link}}" hover-class="navigator-hover">
            <image src="{{item.url}}" class="banner" style="width:100%;height:370rpx;"></image>
          </navigator>
        </swiper-item>
      </block>
    </swiper>
    <view class="swiper-dots">
      <block wx:for="{{slideDots}}" wx:key="i" wx:for-index="i" wx:for-item="item">
        <view wx:if="{{item.cur}}" class="active"></view>
        <view wx:else></view>
      </block>
    </view>
    <view class="title_s">
      <text>评级新品展现
      </text>
    </view>
    <view class="row">
      <view class="item_size">
        <image class="img_show" src="http://ytgrading.com/coin/index/pubimage/showCase/1600169b-cba2-4aec-87c3-1285b178c5fb.jpg" bindtap="clickd"></image>
